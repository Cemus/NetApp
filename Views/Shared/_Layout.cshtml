@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/css/app.css">
    <title>@ViewData["Title"]</title>
</head>

<body>
    <div id="app">
        @await Html.PartialAsync("_Header")
        <main>
            @RenderBody()
        </main>
        @await Html.PartialAsync("_Footer")
    </div>
</body>

</html>